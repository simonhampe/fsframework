<?xml version="1.0" encoding="UTF-8"?>
<fsfw:polyglotstringtable xmlns:fsfw="http://www.fsmathe.uni-kl.de/fsframework" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.fsmathe.uni-kl.de/fsframework ../schema/PolyglotStringTable.xsd " id="FSFW_PST_EXAMPLE">
  <description>An example polyglot string table for fsframework</description>
  <languagetable>
    <language>
      <id>de</id>
      <description>Deutsch</description>
    </language>
    <language>
    	<id>en</id>
    	<description>English</description>
    </language>
    <language>
    	<id>wu</id>
    	<description>Wuselsch</description>
    </language>
  </languagetable>
  <!-- This table will yield the group hierarchy (root) -> (fs) -> (fs.example)  / (fs.moreexamples)-->
  <polyglotstring id="EX01" group="fs.example">
  	<variant lang="de">Erstes %.4f Beispiel: %s</variant>
  	<variant lang="en">First example</variant>
  </polyglotstring>
  <polyglotstring id="EX02" group="fs.moreexamples">
  	<variant lang="wu">Wusel</variant>
  	<variant lang="xy">This is an example for a language which is actually used, but not listed in the language table</variant>
  </polyglotstring>
</fsfw:polyglotstringtable>
